{
  "name": "vfile-reporter-json",
  "version": "2.0.2",
  "description": "vfile utility to create a JSON report for a file",
  "license": "MIT",
  "keywords": [
    "vfile",
    "vfile-util",
    "vfile-reporter",
    "util",
    "utility",
    "reporter",
    "virtual",
    "file",
    "formatter",
    "format",
    "report",
    "json"
  ],
  "repository": "vfile/vfile-reporter-json",
  "bugs": "https://github.com/vfile/vfile-reporter-json/issues",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/unified"
  },
  "author": "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)"
  ],
  "files": [
    "index.js"
  ],
  "dependencies": {},
  "devDependencies": {
    "browserify": "^17.0.0",
    "nyc": "^15.0.0",
    "prettier": "^2.0.0",
    "remark-cli": "^9.0.0",
    "remark-preset-wooorm": "^8.0.0",
    "tape": "^5.0.0",
    "tinyify": "^3.0.0",
    "vfile": "^4.0.0",
    "xo": "^0.35.0"
  },
  "scripts": {
    "format": "remark . -qfo && prettier . -w --loglevel warn && xo --fix",
    "build-bundle": "browserify . -s vfileReporterJson -o vfile-reporter-json.js",
    "build-mangle": "browserify . -s vfileReporterJson -o vfile-reporter-json.min.js -p tinyify",
    "build": "npm run build-bundle && npm run build-mangle",
    "test-api": "node test",
    "test-coverage": "nyc --reporter lcov tape test.js",
    "test": "npm run format && npm run build && npm run test-coverage"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true,
    "esnext": false,
    "rules": {
      "unicorn/prefer-optional-catch-binding": "off"
    },
    "ignores": [
      "vfile-reporter-json.js"
    ]
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm"
    ]
  }

,"_resolved": "https://registry.npmjs.org/vfile-reporter-json/-/vfile-reporter-json-2.0.2.tgz"
